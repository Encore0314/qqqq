/***************按钮定义******************/
a.b_gray{
	padding-left:18px;
	padding-right:18px;
	padding-bottom:5px;
	padding-top:5px;
	border: 1px solid #CCC;
	color:#FFF;
	font-size:12px;
	font-weight:600;
	cursor:pointer;
	}
/*a.b_gray:active{
	background:#757575;
	}*/
a.b_gray:link, .b_gray:visited{
	background:#999;
	}
a.b_gray:over{
	background:#757575;
	}
.button, button{
	padding-left:18px;
	padding-right:18px;
	padding-bottom:5px;
	padding-top:5px;
	border: 1px solid #f66001;
	color:#FFF;
	font-size:12px;
	font-weight:600;
	cursor:pointer;
	background:#f76d15;

	}
a.button, a.button_2, a.button_3{
	padding-left:18px;
	padding-right:18px;
	padding-bottom:5px;
	padding-top:5px;
	border: 1px solid #f66001;
	font-size:12px;
	font-weight:600;
	cursor:pointer;
	color:#FFF;
	
	}
button:active{
	background:#f05e01;
	}
a.button:link, a.button:visited{

	background:#f76d15;
	}
	
a.button:hover{
	background:#f05e01;
	}
	
	
a.button_2{
	background:url(../img/jiahao.png) no-repeat left;
	}

a.button_2:link, a.button_2:visited{

	background:#f76d15 url(../img/jiahao.png) no-repeat left;
	}
	
a.button_2:hover{
	background:#f05e01 url(../img/jiahao.png) no-repeat left;
	}
	
a.button_3{
	background:url(../img/daochu.png) no-repeat left;
	}
a.button_3:link, a.button_3:visited{

	background:#f76d15 url(../img/daochu.png) no-repeat left;
	}
a.button_3:hover{
	background:#f05e01 url(../img/daochu.png) no-repeat left;
	}
	
.link_return{
	background:none;
	border:none;
	}

.butt_mar{
	margin-left:10px;
	}
.xiala{
	 background:url(img/xiala.png) right no-repeat;
	}
.select{
	width:100%;
	height:60px;
	}
/***************页数定义******************/
.page_box{
	width:80%;
	margin:10px auto;
	}
	
.page_table{
	margin:0 auto;
	}
	
.page_table td{
	padding:10px;
	margin:5px;
	overflow: hidden;
	min-width: 10px;
	}
	
.td_page a{
	display:block;
	width:110%;
	padding:5%;
	height:110%;
	text-align:center;
	font-size:12px;
	color:#333;
	}

.td_page a:link, .td_page a:visited{
	text-decoration:underline;
	color:#f76d15;
	-webkit-border-radius: 2px;
	}
	
.td_page a:hover{
	text-decoration:none;
	color:#FFF;
	background:#f76d15;
	-webkit-border-radius: 2px;
	}
	
/******************色块定义**********************/
.orange{
	width:100%;
	height:2px;
	background:#F66001;
	}
.bg_white{
	width:100%;
	height:auto;
	background:#FFF;
	}
.bg_lv{
	background:#EFF3F4;
	
	}
.bg_gray{
	background:#f3f3f3;
	}
