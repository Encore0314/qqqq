@charset "utf-8";
/* CSS Document */

/***********************字体定义*********************************/

.font_white {
	font-size:14px;
	color:#FFF;  
}
.font_white2 {
	font-size:16px;
	color:#FFF;  
}
.font_title{
	font-size:14px; 
	font-weight: bold;
}
.font_title2{
	font-size:14px; 
	font-weight: bold;
}
.font_blue{
	color:#43B8EE;
	
	}
.font_black{
	font-size:14px;
	}
.font_gray {
  font-size: 14px;
  color: rgb( 180, 180, 180 );
}

.font_gray_02 {
  font-size: 14px;
  color:#b0b0b0;
}

.font_big{
	font-size:32px;
	color:rgb( 92, 92, 92 );
	height:60px;
	line-height:60px;
	width:100%;
	text-align: center;
	}
	
.font_oran{
	color:#f66d16;
	}

strong{
	font-weight:bold;
	}	
/***********************end*********************************/


/***********************链接定义*********************************/

a.link_st01{
	display:inline;
	}
	
a.link_st01:link{
	color:#FFF;
	
	}
a.link_st01:visited{
	color:#333;
	
	}
	
a.link_st01:hover{
	text-decoration:underline;
	
	}
	
a.link_left{
	z-index:-100;
	display:block;
	font-size:14px;
	text-align:center;
	height:35px;
	line-height:35px;	
	border-bottom:1px solid #E1F0FD;
	}
	

	
a.link_left:link{
		 color:#43B8EE;
	}
	
a.link_left:visited{

	 color: rgb( 184, 184, 184 );
	
	}
	
a.link_left:hover{
	color: rgb( 27, 27, 27 );
	background:#D5EEFB;
	}
	
	
	
a.lin_cz{
	text-decoration:none;
	color:#369;
	cursor:pointer;
	}
	
a.lin_cz:link{
	text-decoration:none;
	color:#369;
	}	
	
a.lin_cz:visited{
	text-decoration:none;
	color:#F0F;
	}	

a.lin_cz:hover{
	text-decoration:underline;
	color:#369;
	}

a.link_img{
	display:inline;
	}
a.link_img:link{
	text-decoration:none;
	color:#43B8EE;
	}
a.link_img:visited{
	color:#333;
	text-decoration:none;
	}

a.link_img:hover{
	text-decoration:underline;
	color:#f05e01;
	}

a.link_return:link{
	text-decoration:none;
	color:#f76d15;
	}
a.link_return:visited{
	text-decoration:none;
	color:#000;
	}
a.link_return:hover{
	text-decoration:underline;
	color:#f76d15;
	}
a.tabtitle1, a.tabtitle{
	display:block;
	padding-bottom:2px;
	padding-top:2px;
	padding-left:16px;
	padding-right:16px;
	line-height:30px;
	text-align:center;
	height:30px;
	}
a.tabtitle1{
	position: relative;
	z-index: 100;
	float:left;
	color:#f76d15;
	background:#FFF;
	border-top:2px solid #f76d15;
	border-bottom:2px solid #FFF;
	color:#f76d15;
	}

a.tabtitle{
	float:left;
	border-right:1px solid #eeeff0;
	color:#787878;

	}
